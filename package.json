  "description": "Imagem de PostgresSQL putencializado com http e outras extens√µes fundamentais",
  "main": "index.js",
  "scripts": {
    "build:clean": "node build/clean",
    "build:image": "docker build -t zootakuxy/postgrade:$(cat ./TAG) -t zootakuxy/postgrade:latest .",
    "build:publish": "docker push zootakuxy/postgrade:$(cat ./TAG) & docker push zootakuxy/postgrade:latest",
    "build:publish-tag": "docker push zootakuxy/postgrade:$(cat ./TAG)",
    "build:publish-latest": "docker push zootakuxy/postgrade:latest",
    "tsc": "tsc",
    "win:build": "tsc & npm run build:clean & tsc",
    "lin:build": "tsc ; npm run build:clean ; tsc",
    "win:tsc -w": "tsc & npm run build:clean & tsc -w",
    "lin:tsc -w": "tsc ; npm run build:clean ; tsc -w"
